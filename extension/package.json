    "@langchain/core": "^0.3.76",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@wxt-dev/storage": "^1.1.1",
    "@xenova/transformers": "^2.17.2",
    "@xyflow/react": "^12.8.6",
    "ai": "^5.0.56",
    "async-retry": "^1.3.3",
